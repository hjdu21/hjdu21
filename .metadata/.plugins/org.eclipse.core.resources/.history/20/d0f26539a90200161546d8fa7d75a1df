package step32.exam01;

public abstract class CarFactory {
  
  
  private CarFactory() { }
  
  static CarFactory instance;
  
  public abstract static CarFactory newInstance() {
    if(instance == null) {
      instance = new CarFactory();
    }
    return instance;
  }
  
  public  Car newCar(String name) {
  
  }
}
